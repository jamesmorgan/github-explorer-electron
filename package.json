{
  "name": "github-explorer-electron",
  "productName": "Github Explorer Electron",
  "version": "0.0.1",
  "description": "Cross platform github integration in your desktop menu bar!",
  "license": "MIT",
  "repository": "git@github.com:jamesmorgan/github-explorer-electron.git",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "scripts": {
    "start": "electron .",
    "build": "npm run build-osx & npm run build-linux",
    "build-osx": "electron-packager . --overwrite --out=dist --platform=darwin --icon=./resources/tray/icon.icns --enable-logging",
    "build-linux": "electron-packager . --overwrite --out=dist --platform=linux --icon=./resources/tray/icon.icns --enable-logging"
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "bluebird": "3.4.6",
    "electron-debug": "1.0.1",
    "lodash": "4.16.4",
    "menubar": "5.1.0",
    "moment": "2.15.1",
    "node-notifier": "4.6.1",
    "octonode": "0.7.6"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.4.3",
    "electron-packager": "8.3.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
