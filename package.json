{
  "name": "github-explorer-electron",
  "productName": "Github Explorer Electron",
  "version": "0.0.1",
  "description": "Cross platform github integration in your desktop menu bar!",
  "license": "MIT",
  "repository": "git@github.com:jamesmorgan/github-explorer-electron.git",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "scripts": {
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "pre-start": "webpack --progress --profile --colors --display-error-details --display-cached",
    "start": "npm run watch & electron .",
    "build": "npm run build-osx & npm run build-ng2 & npm run build-linux",
    "build-ng2": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build-osx": "electron-packager . --overwrite --out=dist --platform=darwin --icon=./resources/tray/icon.icns --enable-logging",
    "build-linux": "electron-packager . --overwrite --out=dist --platform=linux --icon=./resources/tray/icon.icns --enable-logging"
  },
  "keywords": [
    "electron-app",
    "electron",
    "github",
    "github-explorer"
  ],
  "dependencies": {
    "bluebird": "3.4.6",
    "electron-debug": "1.0.1",
    "lodash": "4.16.4",
    "menubar": "5.1.0",
    "moment": "2.15.1",
    "node-notifier": "4.6.1",
    "octonode": "0.7.6",


    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.4.3",
    "electron-packager": "8.3.0",

    "es6-shim": "^0.35.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
